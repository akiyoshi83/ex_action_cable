# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore Byebug command history file.
.byebug_history


# config
config/settings.local.yml
config/settings/*.local.yml
config/environments/*.local.yml


### https://raw.github.com/github/gitignore/ac5cd42a926545287b20fac21621b286831cb74e/Global/redis.gitignore

